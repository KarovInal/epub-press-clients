{
  "name": "epub-press-js",
  "version": "0.1.0",
  "description": "Javascript client for building books with EpubPress.",
  "homepage": "https://github.com/haroldtreen/epub-press-clients#readme",
  "baseUrl": "https://epub.press",
  "main": "build/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "export ENV=test && open http://localhost:5001/tests && webpack-dev-server",
    "build": "export ENV=development && webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haroldtreen/epub-press-clients.git"
  },
  "keywords": [
    "epub",
    "publishing",
    "productivity",
    "client",
    "epubpress",
    "ebooks",
    "content",
    "extraction"
  ],
  "author": "EpubPress",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/haroldtreen/epub-press-clients/issues"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "fetch-mock": "^5.0.3",
    "mocha": "^2.5.3",
    "mocha-loader": "^0.7.1",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "file-saver": "^1.3.2",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "whatwg-fetch": "^1.0.0"
  }
}
